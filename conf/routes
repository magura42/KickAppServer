# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~



# person
GET         /person                 @controllers.PersonController.listPersons
GET         /person/:personId       @controllers.PersonController.getPerson(personId: Int)
#PUT         /user                   controllers.UserController.updateUser
#DELETE      /user/:userId           controllers.UserController.deleteUser(userId: Long)
#POST        /user                   controllers.UserController.newUser

#POST     /books                           controllers.Application.saveBook


