# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~



# user
GET         /user                   controllers.UserController.listUsers
#GET         /user/:userId           controllers.UserController.getUser(userId: Long)
#PUT         /user                   controllers.UserController.updateUser
#DELETE      /user/:userId           controllers.UserController.deleteUser(userId: Long)
#POST        /user                   controllers.UserController.newUser

#POST     /books                           controllers.Application.saveBook


